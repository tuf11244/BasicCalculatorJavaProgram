import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		System.out.println("Hello World");
		int [] arr = {25,26,28,30,55};
		Scanner console = new Scanner(System.in);
		System.out.println("Please enter the element that you are looking for");
		int element = console.nextInt();
		int i = 0;
		while(i < arr.length){
		    if(element == arr[i]){
		        System.out.println("Your element is at the index " + i);
		        break;
		    }
		    else{
		        System.out.println("The element is not present");
		    }
		   i++;
		    }
		  
		}
}
